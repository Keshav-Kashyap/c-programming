#include <stdio.h>

int main() {
    int num1, num2, result;

    // Input two integers
    printf("Enter first number: ");
    scanf("%d", &num1);

    printf("Enter second number: ");
    scanf("%d", &num2);

    // Perform bitwise AND operation
    result = num1 & num2;

    // Print the result
    printf("Result of %d & %d = %d\n", num1, num2, result);

    return 0;
}
